<Window x:Class="yad2.existingUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:yad2"
        mc:Ignorable="d"
        Title="existingUser" Height="200" Width="400">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="19" Margin="98,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="254"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="19" Margin="98,90,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="254"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="21,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="23"><Run Text="Username"/><Run FlowDirection="RightToLeft" Text=":"/></TextBlock>
        <TextBlock x:Name="textBlock1" HorizontalAlignment="Left" Margin="21,90,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="19" Width="82"><Run FlowDirection="RightToLeft" Text="P"/><Run Text="assword:"/></TextBlock>
        <Button x:Name="button" Content="Log in" HorizontalAlignment="Left" Margin="152,131,0,0" VerticalAlignment="Top" Width="119" Click="LogIn" Height="30"/>
    </Grid>
</Window>
